<div>

Welcome, <%= @user.first_name %> <%= @user.last_name%>!<br>
You are logged in under <%= @user.email %> <br>
<%= @user.current_city %><br>
<!-- <%= @user.email %><br> -->
Member Since: <%= @user.created_at %><br>
<!-- <%= link_to "Edit", edit_user_path(@user) %> -->

<button type="button" class="btn btn-default btn-xs"><%= link_to "Edit", edit_user_path(@user) %></button>

</div>

<div class="container">
  <h4><%= @user.first_name %> has contributed the following posts:</h4>
  <% @posts.each do |post| %>
    <ul>
      <li><%= link_to post.title, posts_path(post) %>
    </ul>
  <% end %>

</div>
